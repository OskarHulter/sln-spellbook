---
import { ViewTransitions } from 'astro:transitions'
const title = 'SLN Spellbook'
const description = 'Workflow nirvana for the modern spellcaster.'
---

<link
  rel='icon'
  type='image/svg+xml'
  href='/favicon.svg'
/>
<meta
  name='generator'
  content={Astro.generator}
/>
<!-- Primary Meta Tags -->
<title>{title}</title>
<meta
  name='title'
  content={title}
/>
<meta
  name='description'
  content={description}
/>

<ViewTransitions />
